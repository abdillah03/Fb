import java.util.Scanner;


public class poladeret {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("INPUT :");
		int a=sc.nextInt();
		System.out.println("OUTPUT :");
		for (int b=0;b<a;b++){
			System.out.print("+---");
		}
		System.out.print("+");
				
		

	}

}
